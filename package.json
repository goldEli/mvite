{
  "name": "mvite",
  "version": "1.0.2",
  "description": "Web dev tool via ES Module",
  "scripts": {
    "watch-server": "nodemon --watch 'src/**/*' -e ts,tsx --exec ts-node ./src/index.ts",
    "dev": "tsc -w",
    "build": "rimraf dist && tsc"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mvite": "./bin/mvite.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goldEli/mvite.git"
  },
  "files": [
    "bin",
    "src",
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/goldEli/mvite/issues"
  },
  "homepage": "https://github.com/goldEli/mvite#readme",
  "dependencies": {
    "@babel/core": "^7.12.8",
    "@babel/parser": "^7.12.7",
    "@babel/plugin-transform-react-jsx": "^7.12.7",
    "@babel/types": "^7.12.7",
    "@types/es-module-lexer": "^0.3.0",
    "@types/koa": "^2.11.6",
    "@types/koa-static": "^4.0.1",
    "@types/node": "^14.14.9",
    "babel-plugin-transform-commonjs-es2015-modules": "^4.0.1",
    "chalk": "^4.1.0",
    "es-module-lexer": "^0.3.26",
    "koa": "^2.13.0",
    "koa-static": "^5.0.0",
    "magic-string": "^0.25.7",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  }
}
